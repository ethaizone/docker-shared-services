# Example .env file for postgres-db-refresh
# Remote DB connection
REMOTE_DB_HOST=your.remote.host
REMOTE_DB_PORT=5432
REMOTE_DB_USER=youruser
REMOTE_DB_PASSWORD=yourpassword
REMOTE_DB_NAME=remotedb

# Local DB connection
LOCAL_DB_HOST=localhost
LOCAL_DB_PORT=5432
LOCAL_DB_USER=yourlocaluser
LOCAL_DB_PASSWORD=yourlocalpassword
LOCAL_DB_NAME=localdb

# Tables to skip (space-separated)
SKIP_TABLES="big_table_to_skip another_large_table"
# Chunk size for bulk insert
CHUNK_SIZE=10000
